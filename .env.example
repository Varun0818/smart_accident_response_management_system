# Copy this file to .env and fill values for local development.
# Do NOT commit real secrets to git. Use environment variables in production.

# Django secret key
DJANGO_SECRET_KEY=django-insecure-your-secret-key-here

# Debug mode for local testing (True/False)
DJANGO_DEBUG=True

# Comma-separated allowed hosts, e.g. localhost,127.0.0.1
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key
